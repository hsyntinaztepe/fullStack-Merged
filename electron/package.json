{
  "name": "electron-ol-geojson",
  "version": "1.0.0",
  "type": "module",
  "main": "src/electron.js",
  "private": true,
  "scripts": {
    "dev": "vite",
    "start": "concurrently -k -s first \"vite\" \"set VITE_DEV_SERVER_URL=http://localhost:5173&& wait-on http://localhost:5173 && electron .\"",
    "build": "vite build",
    "electron:dist": "cross-env NODE_ENV=production electron ."
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.8.0",
    "ol": "^10.6.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^37.3.1",
    "vite": "^7.1.3",
    "wait-on": "^7.2.0"
  }
}
